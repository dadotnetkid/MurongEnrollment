@{
    Html.DevExpress().CallbackPanel(cbp =>
    {
        cbp.Name = "cbpGradeReport";
        cbp.CallbackRouteValues = new { Action = "GradeReportPartial", Controller = "Grade" };
        cbp.SetContent(() =>
        {

            if (Model != null)
            {
                Html.DevExpress().WebDocumentViewer(settings =>
                {
                    settings.Height = System.Web.UI.WebControls.Unit.Pixel(700);
                    settings.Name = "WebDocumentViewer";
                }).Bind(Model).GetHtml();
            }
        });

    }).GetHtml();

}
