@{
    Html.DevExpress().CallbackPanel(cbp =>
    {
        cbp.Name = "cbpSchedulePrintPreviewPartial";
        cbp.CallbackRouteValues = new { Controller = "Schedules", Action = "SchedulePrintPreviewPartial" };
        cbp.SetContent(() =>
        {
            if (Model != null)
            {
                Html.DevExpress().WebDocumentViewer(settings =>
                {
                    settings.Height = System.Web.UI.WebControls.Unit.Pixel(500);
                    settings.Name = "WebDocumentViewer";
                }).Bind(Model).GetHtml();
            }

        });

    }).GetHtml();
}
